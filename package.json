{
  "name": "outline-quick-notes-web",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "dev-build": "vite build --mode staging --watch",
    "extension-build": "rollup --config rollup-extension.config.js ",
    "extension-build-watch": "yarn extension-build --watch",
    "zip": "zip -r build.zip dist/",
    "build-final": "vite build  && yarn extension-build",
    "build-n-zip": "yarn build-final &&  yarn zip",
    "prepare": "husky install"
  },
  "dependencies": {
    "highlight.js": "^11.4.0",
    "lodash.omit": "^4.5.0",
    "mitt": "^3.0.0",
    "quill": "^1.3.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "husky": "^7.0.0",
    "prettier": "^2.6.0",
    "pretty-quick": "^3.1.3",
    "rollup-plugin-copy-watch": "^0.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.49.0",
    "vite": "^2.7.2"
  }
}
